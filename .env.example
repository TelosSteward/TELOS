# TELOS Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# Required: Mistral API Key for embeddings and LLM
MISTRAL_API_KEY=your_mistral_api_key_here

# Optional: OpenAI API Key (for gateway OpenAI provider)
OPENAI_API_KEY=your_openai_api_key_here

# Gateway Configuration
TELOS_GATEWAY_PORT=8000
TELOS_GATEWAY_HOST=0.0.0.0

# CORS Configuration (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:8501,http://localhost:3000,https://beta.telos-labs.ai

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=10

# Supabase Configuration (for governance delta transmission)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_KEY=your-anon-or-service-role-key

# Logging
LOG_LEVEL=INFO
